        """
      bytes = Bytes(79, 115, 116, 114, 105, 99, 104, 46, 112, 107, 108, 10, 83, 119, 97, 108, 108, 111, 119, 46, 112, 107, 108, 10)
    }
    new {
      uri = "projectpackage://localhost:0/birds@0.5.0#/"
